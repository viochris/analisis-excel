{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "R-Debugger",
      "name": "Launch R-Workspace",
      "request": "launch",
      "debugMode": "workspace",
      "workingDirectory": ""
    },
    {
      "type": "R-Debugger",
      "name": "Debug R-File",
      "request": "launch",
      "debugMode": "file",
      "workingDirectory": "",
      "file": ""
    },
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ]
}
